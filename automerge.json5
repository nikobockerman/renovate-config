{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      description: "Automerge: all updates",
      matchUpdateTypes: [
        "major",
        "minor",
        "patch",
        "digest",
        "lockFileMaintenance",
      ],
      automerge: true,
    },
  ],
}
